; EFI error 

EFI_ERROR               = 0x8000000000000000
EFI_SUCCESS             = 0
EFI_LOAD_ERROR          = EFI_ERROR or 1
EFI_INVALID_PARAMETER   = EFI_ERROR or 2
EFI_UNSUPPORTED         = EFI_ERROR or 3
EFI_BAD_BUFFER_SIZE     = EFI_ERROR or 4
EFI_BUFFER_TOO_SMALL    = EFI_ERROR or 5
EFI_NOT_READY           = EFI_ERROR or 6
EFI_DEVICE_ERROR        = EFI_ERROR or 7
EFI_WRITE_PROTECTED     = EFI_ERROR or 8
EFI_OUT_OF_RESOURCES    = EFI_ERROR or 9
EFI_VOLUME_CORRUPTED    = EFI_ERROR or 10
EFI_VOLUME_FULL         = EFI_ERROR or 11
EFI_NO_MEDIA            = EFI_ERROR or 12
EFI_MEDIA_CHANGED       = EFI_ERROR or 13
EFI_NOT_FOUND           = EFI_ERROR or 14
EFI_ACCESS_DENIED       = EFI_ERROR or 15
EFI_NO_RESPONSE         = EFI_ERROR or 16
EFI_NO_MAPPING          = EFI_ERROR or 17
EFI_TIMEOUT             = EFI_ERROR or 18
EFI_NOT_STARTED         = EFI_ERROR or 19
EFI_ALREADY_STARTED     = EFI_ERROR or 20
EFI_ABORTED             = EFI_ERROR or 21
EFI_ICMP_ERROR          = EFI_ERROR or 22
EFI_TFTP_ERROR          = EFI_ERROR or 23
EFI_PROTOCOL_ERROR      = EFI_ERROR or 24

__EFI_ERROR               du "EFI_ERROR", 0
__EFI_SUCCESS             du "EFI_SUCCESS", 0
__EFI_LOAD_ERROR          du "EFI_LOAD_ERROR", 0
__EFI_INVALID_PARAMETER   du "EFI_INVALID_PARAMETER", 0
__EFI_UNSUPPORTED         du "EFI_UNSUPPORTED", 0
__EFI_BAD_BUFFER_SIZE     du "EFI_BAD_BUFFER_SIZE", 0
__EFI_BUFFER_TOO_SMALL    du "EFI_BUFFER_TOO_SMALL", 0
__EFI_NOT_READY           du "EFI_NOT_READY", 0
__EFI_DEVICE_ERROR        du "EFI_DEVICE_ERROR", 0
__EFI_WRITE_PROTECTED     du "EFI_WRITE_PROTECTED", 0
__EFI_OUT_OF_RESOURCES    du "EFI_OUT_OF_RESOURCES", 0
__EFI_VOLUME_CORRUPTED    du "EFI_VOLUME_CORRUPTED", 0
__EFI_VOLUME_FULL         du "EFI_VOLUME_FULL", 0
__EFI_NO_MEDIA            du "EFI_NO_MEDIA", 0
__EFI_MEDIA_CHANGED       du "EFI_MEDIA_CHANGED", 0
__EFI_NOT_FOUND           du "EFI_NOT_FOUND", 0
__EFI_ACCESS_DENIED       du "EFI_ACCESS_DENIED", 0
__EFI_NO_RESPONSE         du "EFI_NO_RESPONSE", 0
__EFI_NO_MAPPING          du "EFI_NO_MAPPING", 0
__EFI_TIMEOUT             du "EFI_TIMEOUT", 0
__EFI_NOT_STARTED         du "EFI_NOT_STARTED", 0
__EFI_ALREADY_STARTED     du "EFI_ALREADY_STARTED", 0
__EFI_ABORTED             du "EFI_ABORTED", 0
__EFI_ICMP_ERROR          du "EFI_ICMP_ERROR", 0
__EFI_TFTP_ERROR          du "EFI_TFTP_ERROR", 0
